        {
          "type": "hardcorequesting:detect",
          "description": {
            "text": "Salt",
            "isTranslationKey": false
          },
          "longDescription": {
            "text": "The dust on T4L0S has a high salt concentration. Salt can be used to cure and preserve food, rendering it safe for consumption. Sift some dust to collect salt.",
